{
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "async": true,
      "logconsole": {
        "type": "ColoredConsole"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "logconsole"
      }
    ]
  },
  "SWAPI": {
    "ApiRootUrl": "https://swapi.py4e.com/api/",
    "MaxDegreeOfParallelism": 2
  }
}